{
  "name": "leafcast_pi",
  "version": "0.0.1",
  "description": "backend node pi client for leafcast service",
  "homepage": "https://github.com/mrjackwills/leafcast_pi",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --clearCache && jest --setupFiles dotenv/config --verbose --watch --detectOpenHandles",
    "testAllc": "jest --clearCache && jest --setupFiles dotenv/config --verbose --watchAll --detectOpenHandles --collectCoverage",
    "serve": "nodemon dist/index.js",
    "release": "node ts-builder/index.js",
    "lint": "eslint --ext .ts --ignore-pattern  *.spec.ts* --ignore-pattern '*/build/*' ./src",
    "dockerBuild": "npm install && tsc --project tsconfig.build.json && npm prune --production && npm cache clean --force",
    "build": "tsc --project tsconfig.build.json"
  },
  "author": "Jack Wills",
  "dependencies": {
    "axios": "^0.25.0",
    "dotenv": "^14.3.2",
    "node-cron": "^3.0.0",
    "pi-camera-connect": "^0.3.4",
    "secure-json-parse": "^2.4.0",
    "sharp": "^0.29.3",
    "winston": "^3.5.0",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@types/node-cron": "^3.0.1",
    "@types/sharp": "^0.29.5",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-security-node": "^1.1.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
